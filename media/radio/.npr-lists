}
    function loadXMLDoc(index, url, responseCallback)
    {
        arrayOfRequests[index] = null;
        if (window.XMLHttpRequest)
          {// code for IE7, Firefox, Mozilla, etc.
            arrayOfRequests[index]=new XMLHttpRequest();
          }
        else if (window.ActiveXObject)
          {// code for IE5, IE6
            arrayOfRequests[index]=new ActiveXObject("Microsoft.XMLHTTP");
          }
        if (arrayOfRequests[index] != null)
          {
            arrayOfRequests[index].onreadystatechange=responseCallback;
            arrayOfRequests[index].open("GET",url,true);
            arrayOfRequests[index].send(null);

          }
        else
          {
          alert("Your browser does not support XMLHTTP.");
          }


    }

function buildTable(name, showAddInfo, additionalName)
{
    if(typeof(showAddInfo) == 'undefined') {
        showAddInfo = true;
    }
    var xmlhttp = arrayOfRequests[name];
    if(!isReady(xmlhttp)) {
        return;
    }
    var table = '<table border="1" width="100%"><tr><th>TITLE</th><th>ID</th>';
    if(showAddInfo) {
        table += '<th>Additional Info</th>';
    }
    table += '</tr>';
    var items = xmlhttp.responseXML.getElementsByTagName('item');
    var itemLength = items.length; // cache total length
    for(var i=0;i<itemLength;i++) {
        table += addToTable(items[i], showAddInfo);
    }
    if(typeof(additionalName) != 'undefined')
    {
        // throw the second group in here, too
        var additionalXmlhttp = arrayOfRequests[additionalName];
        if(isReady(additionalXmlhttp, true)) {
            items = additionalXmlhttp.responseXML.getElementsByTagName('item');
            itemLength = items.length;
            for(var i=0;i<itemLength;i++) {
                table += addToTable(items[i], showAddInfo);
            }
        }
    }
    table += '</table>';
    stopLoad();
    document.getElementById(name+'Content').innerHTML=table;
}

function isReady(xmlhttp, quiet) {
    if(typeof(quiet) == 'undefined') {
        quiet = false;
    }
    if(xmlhttp.readyState!=4) {
        return false;
    }
    else if(xmlhttp.status!=200) {
        if(!quiet) {
            alert("Problem retrieving XML data");
        }
        return false;
    }
    return true;
}

function addToTable(item, showAddInfo) {
    var table = '<tr>';
    try {
        table += '<td>' + item.getElementsByTagName('title')[0].firstChild.nodeValue + '</td>';
    }
    catch (er) {
        table += '<td> </td>';
    }
    try {
        var itemId = item.getAttribute('id');
        table += '<td class="srchID" id="' + itemId + '">' + itemId + '</td>';
    }
    catch (er) {
        table += '<td> </td>';
    }
    if(showAddInfo) {
        try {
            table += '<td>' + item.getElementsByTagName('additionalInfo')[0].firstChild.nodeValue + '</td>';
        }
        catch (er) {
            table += '<td> </td>';
        }
    }
    table += '</tr>';
    return table;
}

function onTopicsResponse() {
    buildTable('topics');
}

function onAggsResponse() {
    if(!isReady(arrayOfRequests['aggregations'], true) || !isReady(arrayOfRequests['columns'], true)) {
        return;
    } 
    buildTable('aggregations', true, 'columns');
}

function onProgramsResponse() {
    buildTable('programs');
}

function onBiosResponse() {
    buildTable('personalities');
}

function onArtistsResponse() {
    buildTable('artists', false);
}

function onBlogsResponse() {
    buildTable('blogs');
}

